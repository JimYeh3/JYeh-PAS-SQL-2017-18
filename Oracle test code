CREATE TABLE Transactions
(OrderID number(10), 
 Transaction number(10),
 TheDate ,
 Item# number(10),
 Inventory number(10),
 CustomerID number(10),
 Points number(10),
 EmpID number(10),
 CostPerItem number(10),
 TotalCost number(10),
 MoneyGiven number(10),
 Change number(10)
);
