CREATE TABLE Items
(
 ItemID number(10),
 ItemName char(10),
 Inventory number (10),
 ItemsType char(10),
 ItemCost number(10),
);


Newest update

create table Item (
  ItemID int not null Primary key,
  "Item Name" varchar(50),
  Inventory int,
  "Item Types" varchar(100),
  "Item Cost" int
)
;

create table Customer (
  CustomerID int not null primary key,
  "Customer FName" varchar(50),
  "Customer LName" varchar(50),
  Room int,
  Floors int,
  Numbers int,
  Street varchar(100),
  City varchar(100),
  Country varchar(50),
  Points int,
  Phone# varchar(30)
)
;

create table Employee (
  EmployeeID int not null primary key,
  Fname varchar(50),
  Lname varchar(50),
  Room int,
  Floors varchar(100),
  Numbers int,
  Street varchar(100),
  City varchar(100),
  Country varchar(50),
  Phone varchar(30),
  Wage int
)
;

create table Transaction (
  Orders int not null,
  Transaction int,
  Dates date,
  "Item#" int,
  Inventory int,
  CustomerID int REFERENCES Customer(CustomerID),
  Points int,
  EmpID int REFERENCES Employee(EmployeeID),
  CostPerItem int,
  TotalCost int,
  "Money Given" int,
  Change int
)
;


